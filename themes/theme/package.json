{
  "name": "boilerplate-theme",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel sync wpstart tailwindwatch",
    "sync": "browser-sync start -p 'wpwebapp.local' --files '**/*.php' 'build/*.js' 'build/*.css'",
    "buildwp": "wp-scripts build",
    "build": "npm-run-all --sequential buildwp tailwindbuild",
    "wpstart": "wp-scripts start",
    "start": "npm-run-all --parallel wpstart tailwindwatch",
    "tailwindbuild": "tailwindcss -i ./src/index.css -o ./build/index.css --minify",
    "tailwindwatch": "tailwindcss -i ./src/index.css -o ./build/index.css --watch --minify",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "t": "npm run test:unit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@wordpress/scripts": "^23.3.0",
    "browser-sync": "^2.27.10",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/core": "^5.9.5",
    "@mantine/dates": "^5.10.2",
    "@mantine/hooks": "^5.9.5",
    "dayjs": "^1.11.7",
    "date-fns": "^2.29.3",
    "@mantine/notifications": "^5.9.5",
    "@tabler/icons": "^1.117.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "gsap": "^3.11.3",
    "whatwg-fetch": "^3.6.2"
  }
}
